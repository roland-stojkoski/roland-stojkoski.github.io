@tailwind base;
@tailwind components;
@tailwind utilities;

html,
body,
label {
	font-family: 'Source Code Pro', monospace;
}

.container-implemented {
	@apply prose prose-slate container mx-auto mt-5 px-5;
}

.container-implemented h1 {
	@apply text-center pt-5;
}
